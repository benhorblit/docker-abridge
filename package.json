{
	"name": "docker-abridge",
	"description": "A tool for managing a docker-compose environment allowing easy enabling/disabling of services.",
	"version": "0.2.1",
	"author": "Ben Horblit",
	"bin": {
		"docker-abridge": "./bin/run"
	},
	"bugs": "https://github.com/benhorblit/docker-abridge/issues",
	"dependencies": {
		"@oclif/command": "^1.5.19",
		"@oclif/config": "^1.13.3",
		"@oclif/plugin-help": "^2.2.3",
		"chalk": "^3.0.0",
		"execa": "^4.0.0",
		"execa-output": "^2.0.0",
		"js-yaml": "^3.13.1",
		"listr": "^0.14.3",
		"rxjs": "^6.5.4"
	},
	"devDependencies": {
		"@oclif/dev-cli": "^1.22.2",
		"babel-eslint": "^10.0.3",
		"eslint": "^6.1.0",
		"eslint-config-airbnb-base": "^14.0.0",
		"eslint-config-oclif": "^3.1.0",
		"eslint-config-prettier": "^6.9.0",
		"eslint-plugin-import": "^2.20.0",
		"eslint-plugin-prettier": "^3.1.2",
		"globby": "^10.0.2",
		"prettier": "^1.19.1"
	},
	"engines": {
		"node": ">=12.0.0"
	},
	"files": [
		"/bin",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json",
		"/src"
	],
	"homepage": "https://github.com/benhorblit/docker-abridge",
	"keywords": [
		"oclif"
	],
	"license": "Apache-2.0",
	"main": "src/index.js",
	"oclif": {
		"commands": "./src/commands",
		"bin": "docker-abridge",
		"plugins": [
			"@oclif/plugin-help"
		]
	},
	"repository": "benhorblit/docker-abridge",
	"scripts": {
		"postpack": "rm -f oclif.manifest.json",
		"posttest": "eslint .",
		"prepack": "oclif-dev manifest && oclif-dev readme",
		"test": "echo NO TESTS",
		"version": "oclif-dev readme && git add README.md",
		"lint": "eslint .",
		"lintfix": "eslint . --fix"
	}
}
